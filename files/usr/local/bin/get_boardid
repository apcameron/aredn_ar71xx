#!/bin/sh
# Return the model of the device

if [ ! -f /tmp/sysinfo/boardid ]; then
CFG=/etc/board.json
name=""

. /usr/share/libubox/jshn.sh

[ -f $CFG ] || exit 1
json_init
json_load "$(cat ${CFG})"
json_select model
json_get_vars name

echo "$name" > /tmp/sysinfo/boardid

fi

cat /tmp/sysinfo/boardid
